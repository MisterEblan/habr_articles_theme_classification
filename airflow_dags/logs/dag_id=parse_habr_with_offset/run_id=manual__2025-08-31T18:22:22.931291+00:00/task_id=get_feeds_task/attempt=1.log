     break;\\n            }\\n        }\\n\\n        if (found) {\\n            handler(ctx);\\n        } else {\\n            std::cout << \"HTTP 403 Forbidden: Role \\'\" << requiredRole << \"\\' is required.\" << std::endl;\\n        }\\n    };\\n}\\n\\n\\nvoid viewDashboard(const RequestContext& ctx) {\\n    std::cout << \"User \" << ctx.userId << \" is viewing the dashboard.\" << std::endl;\\n}\\n\\nvoid deleteUser(const RequestContext& ctx) {\\n    std::cout << \"User \" << ctx.userId << \" is deleting another user. High privilege operation!\" << std::endl;\\n}\\n\\nint main() {\\n\\n    auto protectedViewDashboard = requireRole(\"user\", viewDashboard);\\n    auto protectedDeleteUser = requireRole(\"admin\", deleteUser);\\n\\n\\n    RequestContext userContext = {101, {\"user\"}};\\n    RequestContext adminContext = {202, {\"user\", \"admin\"}};\\n    RequestContext guestContext = {303, {}};\\n\\n    std::cout << \"--- Attempting as regular user ---\" << std::endl;\\n    protectedViewDashboard(userContext); \\n    protectedDeleteUser(userContext); \\n\\n    std::cout << \"\\\\n--- Attempting as admin ---\" << std::endl;\\n    protectedViewDashboard(adminContext); \\n    protectedDeleteUser(adminContext); \\n\\n    std::cout << \"\\\\n--- Attempting as guest ---\" << std::endl;\\n    protectedViewDashboard(guestContext);\\n    protectedDeleteUser(guestContext); \\n\\n    return 0;\\n} –ü—Ä–æ–±–ª–µ–º–∞ ‚Ññ7 ‚Äì –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–∞—è –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏—è –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ –ò–∑–ª–∏—à–Ω–µ –ø–æ–¥—Ä–æ–±–Ω—ã–µ —Å–æ–æ–±—â–µ–Ω–∏—è –æ–± –æ—à–∏–±–∫–∞—Ö, –≤–∫–ª—é—á–µ–Ω–Ω—ã–µ –æ—Ç–ª–∞–¥–æ—á–Ω—ã–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã, –æ—Ç–∫—Ä—ã—Ç—ã–µ –ø–æ—Ä—Ç—ã. –†–µ—à–µ–Ω–∏–µ –ê: –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ Security Headers –°—É—Ç—å: \\xa0–ù–∞—Å—Ç—Ä–æ–∏—Ç—å –≤–µ–±-—Å–µ—Ä–≤–µ—Ä –Ω–∞ –æ—Ç–ø—Ä–∞–≤–∫—É –∑–∞–≥–æ–ª–æ–≤–∫–æ–≤\\xa0Strict-Transport-Security,\\xa0Content-Security-Policy\\xa0–∏ –¥—Ä. –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–ü–µ—Ä–µ–∫–ª–∞–¥—ã–≤–∞–µ—Ç —á–∞—Å—Ç—å –∑–∞—â–∏—Ç—ã –Ω–∞ –±—Ä–∞—É–∑–µ—Ä –∫–ª–∏–µ–Ω—Ç–∞. –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–¢—Ä–µ–±—É–µ—Ç —Ç—â–∞—Ç–µ–ª—å–Ω–æ–π –Ω–∞—Å—Ç—Ä–æ–π–∫–∏. –ó–∞—â–∏—â–∞–µ—Ç –≤ –æ—Å–Ω–æ–≤–Ω–æ–º –±—Ä–∞—É–∑–µ—Ä–Ω—ã—Ö –∫–ª–∏–µ–Ω—Ç–æ–≤. –†–µ—à–µ–Ω–∏–µ –ë: –°—Ç–∞–Ω–¥–∞—Ä—Ç–∏–∑–∞—Ü–∏—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –æ—à–∏–±–æ–∫ –°—É—Ç—å: \\xa0–í–º–µ—Å—Ç–æ \"–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å \\'admin\\' –Ω–µ –Ω–∞–π–¥–µ–Ω\" –≤—Å–µ–≥–¥–∞ –≤–æ–∑–≤—Ä–∞—â–∞—Ç—å \"–ù–µ–≤–µ—Ä–Ω—ã–π –ª–æ–≥–∏–Ω –∏–ª–∏ –ø–∞—Ä–æ–ª—å\". –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–ù–µ –¥–∞–µ—Ç –∑–ª–æ—É–º—ã—à–ª–µ–Ω–Ω–∏–∫—É –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –¥–ª—è –ø–µ—Ä–µ–±–æ—Ä–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π. –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–ú–æ–∂–µ—Ç –Ω–µ–º–Ω–æ–≥–æ —É—Å–ª–æ–∂–Ω–∏—Ç—å –æ—Ç–ª–∞–¥–∫—É. –ü—Ä–æ–±–ª–µ–º–∞ ‚Ññ8 ‚Äì –ò–Ω—ä–µ–∫—Ü–∏–∏ (SQL, NoSQL, OS) –ü—Ä–æ–±–ª–µ–º–∞ –≤–æ–∑–Ω–∏–∫–∞–µ—Ç, –∫–æ–≥–¥–∞ –¥–∞–Ω–Ω—ã–µ –æ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è —Å—Ç–∞–Ω–æ–≤—è—Ç—Å—è —á–∞—Å—Ç—å—é –∫–æ–º–∞–Ω–¥—ã –¥–ª—è –±–∞–∑—ã –¥–∞–Ω–Ω—ã—Ö. –†–µ—à–µ–Ω–∏–µ –ê: –ü–∞—Ä–∞–º–µ—Ç—Ä–∏–∑–æ–≤–∞–Ω–Ω—ã–µ –∑–∞–ø—Ä–æ—Å—ã (Prepared Statements) –ö–∞–∫ —ç—Ç–æ —Ä–∞–±–æ—Ç–∞–µ—Ç? \\xa0–ß—Ç–æ–±—ã —ç—Ç–æ–≥–æ –∏–∑–±–µ–∂–∞—Ç—å, –Ω—É–∂–Ω–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ API, –∫–æ—Ç–æ—Ä—ã–µ —Ä–∞–∑–¥–µ–ª—è—é—Ç –∫–æ–º–∞–Ω–¥—É –∏ –¥–∞–Ω–Ω—ã–µ. –≠—Ç–æ –∫–∞–∫ –ø–µ—Ä–µ–¥–∞—Ç—å –≤ —Ñ—É–Ω–∫—Ü–∏—é –∞—Ä–≥—É–º–µ–Ω—Ç—ã: —Å–∞–º–∞ –∫–æ–º–∞–Ω–¥–∞ –æ—Ç–¥–µ–ª—å–Ω–æ, –¥–∞–Ω–Ω—ã–µ –æ—Ç–¥–µ–ª—å–Ω–æ. –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö –ø—Ä–æ—Å—Ç–æ –Ω–µ –ø–æ–∑–≤–æ–ª–∏—Ç –¥–∞–Ω–Ω—ã–º —Å—Ç–∞—Ç—å –∏—Å–ø–æ–ª–Ω—è–µ–º—ã–º –∫–æ–¥–æ–º. –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–≠—Ç–æ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∏ —Å–∞–º—ã–π –Ω–∞–¥–µ–∂–Ω—ã–π —Å–ø–æ—Å–æ–± –∑–∞—â–∏—Ç—ã –æ—Ç SQL-–∏–Ω—ä–µ–∫—Ü–∏–π. –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–¢—Ä–µ–±—É–µ—Ç –æ—Ç —Ä–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫–∞ –≤—Å–µ–≥–¥–∞ –ø–æ–º–Ω–∏—Ç—å –æ–± —ç—Ç–æ–º –ø—Ä–∞–≤–∏–ª–µ. –†–µ—à–µ–Ω–∏–µ –ë: –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ ORM –ö–∞–∫ —ç—Ç–æ —Ä–∞–±–æ—Ç–∞–µ—Ç? \\xa0–ò–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç—ã –≤—Ä–æ–¥–µ Hibernate –∏–ª–∏ Entity Framework –æ–±—ã—á–Ω–æ –∏—Å–ø–æ–ª—å–∑—É—é—Ç –ø–∞—Ä–∞–º–µ—Ç—Ä–∏–∑–∞—Ü–∏—é –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é. –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–°–∏–ª—å–Ω–æ —É–ø—Ä–æ—â–∞–µ—Ç –∂–∏–∑–Ω—å –∏ –¥–∞–µ—Ç —Ö–æ—Ä–æ—à–∏–π —É—Ä–æ–≤–µ–Ω—å –∑–∞—â–∏—Ç—ã \"–∏–∑ –∫–æ—Ä–æ–±–∫–∏\". –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–ù—É–∂–Ω–æ –±—ã—Ç—å –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω—ã–º, –∫–æ–≥–¥–∞ –ø–∏—à–µ—à—å \"—Å—ã—Ä—ã–µ\" SQL-–∑–∞–ø—Ä–æ—Å—ã —á–µ—Ä–µ–∑ ORM ‚Äî —Ç–∞–º –∑–∞—â–∏—Ç–∞ –º–æ–∂–µ—Ç –Ω–µ —Å—Ä–∞–±–æ—Ç–∞—Ç—å. –ü—Ä–æ–±–ª–µ–º–∞ ‚Ññ9 ‚Äì –ù–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ–µ –ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –∏ –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ –ê—Ç–∞–∫–∞ –ø—Ä–æ–∏–∑–æ—à–ª–∞, –Ω–æ –Ω–∏–∫—Ç–æ —ç—Ç–æ–≥–æ –Ω–µ –∑–∞–º–µ—Ç–∏–ª, –∏ —Ä–∞—Å—Å–ª–µ–¥–æ–≤–∞—Ç—å –Ω–µ—á–µ–≥–æ. –†–µ—à–µ–Ω–∏–µ –ê: –¶–µ–Ω—Ç—Ä–∞–ª–∏–∑–æ–≤–∞–Ω–Ω–æ–µ —Ö—Ä–∞–Ω–∏–ª–∏—â–µ –ª–æ–≥–æ–≤ (ELK, Splunk) –°—É—Ç—å: \\xa0–°–æ–±–∏—Ä–∞—Ç—å –ª–æ–≥–∏ —Å–æ –≤—Å–µ—Ö —Å–µ—Ä–≤–∏—Å–æ–≤ –≤ –µ–¥–∏–Ω—É—é —Å–∏—Å—Ç–µ–º—É. –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–ü–æ–∑–≤–æ–ª—è–µ—Ç –∫–æ—Ä—Ä–µ–ª–∏—Ä–æ–≤–∞—Ç—å —Å–æ–±—ã—Ç–∏—è –∏ –≤–∏–¥–µ—Ç—å –ø–æ–ª–Ω—É—é –∫–∞—Ä—Ç–∏–Ω—É. –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–¢—Ä–µ–±—É–µ—Ç —Ä–∞–∑–≤–µ—Ä—Ç—ã–≤–∞–Ω–∏—è –∏ –ø–æ–¥–¥–µ—Ä–∂–∫–∏ –æ—Ç–¥–µ–ª—å–Ω–æ–π –∏–Ω—Ñ—Ä–∞—Å—Ç—Ä—É–∫—Ç—É—Ä—ã. –†–µ—à–µ–Ω–∏–µ –ë: –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –æ–ø–æ–≤–µ—â–µ–Ω–∏–π (Alerting) –°—É—Ç—å: \\xa0–ù–∞—Å—Ç—Ä–æ–∏—Ç—å –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ –æ–ø–æ–≤–µ—â–µ–Ω–∏—è –Ω–∞ –∞–Ω–æ–º–∞–ª–∏–∏: 100 –Ω–µ—É–¥–∞—á–Ω—ã—Ö –ø–æ–ø—ã—Ç–æ–∫ –≤—Ö–æ–¥–∞ –∑–∞ –º–∏–Ω—É—Ç—É —Å –æ–¥–Ω–æ–≥–æ IP. –ü—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞: \\xa0–ü—Ä–æ–∞–∫—Ç–∏–≤–Ω–æ–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω–∏–µ –∞—Ç–∞–∫. –ù–µ–¥–æ—Å—Ç–∞—Ç–∫–∏: \\xa0–†–∏—Å–∫ \"—É—Å—Ç–∞–ª–æ—Å—Ç–∏ –æ—Ç –∞–ª–µ—Ä—Ç–æ–≤\" –ø—Ä–∏ –Ω–µ–≤–µ—Ä–Ω–æ–π –Ω–∞—Å—Ç—Ä–æ–π–∫–µ. –ü—Ä–æ–±–ª–µ–º–∞ ‚Ññ10 ‚Äì –ù–µ–±–µ–∑–æ–ø–∞—Å–Ω–∞—è –ø–µ—Ä–µ–∞–¥—Ä–µ—Å–∞—Ü–∏—è API –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –Ω–∞ URL, —É–∫–∞–∑–∞–Ω–Ω—ã–π –≤ –ø–∞—Ä–∞–º–µ—Ç—Ä–µ, —á—Ç–æ –≤–µ–¥–µ—Ç –∫ —Ñ–∏—à–∏–Ω–≥—É. –†–µ—à–µ–Ω–∏–µ –ê: –ë–µ–ª—ã–π —Å–ø–∏—Å–æ–∫ URL –¥–ª—è —Ä–µ–¥–∏—Ä–µ–∫—Ç–∞ –ö–∞–∫ —ç—Ç–æ —Ä–∞–±–æ—Ç–∞–µ—Ç? \\xa0–ü—Ä–æ—Å—Ç–æ–µ –ø—Ä–∞–≤–∏–ª–æ: –¥–µ—Ä–∂–∏–º —É —Å–µ–±—è —Å–ø–∏—Å–æ–∫ \"—Å–≤–æ–∏—Ö\" –¥–æ–º–µ–Ω–æ–≤. –ï—Å–ª–∏ URL –¥–ª—è —Ä–µ–¥–∏—Ä–µ–∫—Ç–∞ –Ω–µ –∏–∑ —ç—Ç–æ